{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "outDir": "dist",
    "types": [
      "vite/client",
    ],
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true
  },
  "include": [
    "components/**/*.vue",
    "layouts/**/*.vue",
    "setup/*.ts",
    "utils/*.js",
    "windi.config.js",
    "utils/props.ts",
  ]
}